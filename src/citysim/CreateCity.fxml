   <children>
      <Button fx:id="buttonCreateCity" layoutX="180.0" layoutY="128.0" mnemonicParsing="false" onAction="#createCity" text="Create city" />
      <TextField fx:id="textfieldCityName" layoutX="142.0" layoutY="61.0" text="Name of new city" />
      <TextField fx:id="textfieldPopulation" layoutX="142.0" layoutY="93.0" text="Initial population" />
      <Text fx:id="createCityText" layoutX="169.0" layoutY="40.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Create new city">
         <font>
            <Font size="14.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
